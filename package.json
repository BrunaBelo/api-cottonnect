{
  "name": "api-cottonnect",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node src/index.ts",
    "start-dev": "NODE_ENV=development ts-node-dev src/index.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "migration-test": "NODE_ENV=test ts-node-dev node_modules/typeorm/cli.js migration:run",
    "test": "NODE_ENV=test jest --runInBand",
    "restart-docker": "docker-compose stop && docker-compose start-dev && docker logs -f cottonnect",
    "populate-locations": "ts-node-dev src/database/seeds/populateLocations.ts",
    "populate-roles": "ts-node-dev src/database/seeds/populateRole.ts",
    "populate-categories": "ts-node-dev src/database/seeds/populateCategories.ts",
    "run-seeds": "concurrently \"yarn populate-locations\" \"yarn populate-roles\" \"yarn populate-categories\""
  },
  "repository": "git@github.com:BrunaBelo/api-cottonnect.git",
